first = Triangle((1, 1), (5, 1), (1, 5))
second = Triangle((3, 1), (1, 3), (3, 3))
third = Triangle((0, 0), (1, 1), (1, 1))
fourth = Triangle((1, 1), (1, 1), (1, 1))
print(abs(first), abs(second), abs(third), abs(fourth))
print(bool(first), bool(second), bool(third), bool(fourth))
print(first < second, first > second, third == fourth, first >= fourth)
print(first in second, second in first, third in second, fourth in first, third in fourth)
print(first & second, second & first, fourth & second, third & first)
